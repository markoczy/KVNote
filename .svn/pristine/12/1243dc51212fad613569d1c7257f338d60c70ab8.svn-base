package mkz.kvnote.settings;

public class VersionInfo
{
	/** The member release. */
	private static int mRelease = 0;
	
	/** The member subrelease. */
	private static int mSubrelease = 1;
	
	/** The member build. */
	private static int mBuild = 1;
	
	/**
	 * Gets the version.
	 *
	 * @return the version
	 */
	public static String getVersion()
	{
		return mRelease+"."+mSubrelease+"."+mBuild;
	}
	
	/**
	 * Gets the release.
	 *
	 * @return the release
	 */
	public static int getRelease()
	{
		return mRelease;
	}
	
	/**
	 * Gets the sub releases.
	 *
	 * @return the sub releases
	 */
	public static int getSubReleases()
	{
		return mSubrelease;
	}
	
	/**
	 * Gets the build.
	 *
	 * @return the build
	 */
	public static int getBuild()
	{
		return mBuild;
	}
	
}
